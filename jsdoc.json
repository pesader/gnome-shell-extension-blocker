{
  "source": {
    "include": ["blocker@pesader.dev"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.jsdoc.md",
    "template": "node_modules/docdash"
  },
  "docdash": {
    "search": true
  }
}

