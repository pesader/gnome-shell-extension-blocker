{
  "source": {
    "include": ["blocker@pesader.dev"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.jsdoc.md",
    "template": "node_modules/docdash"
  },
  "docdash": {
    "search": true,
    "openGraph": {
        "title": "Explore Blocker's codebase and APIs",
        "type": "website",
        "image": "assets/banner-github.png",
        "site_name": "Blocker Developer Documentation",
        "url": "https://pesader.dev/gnome-shell-extension-blocker/"
    },
    "meta": {
        "title": "Blocker Developer Documentation",
        "description": "Explore Blocker's codebase and APIs",
        "keyword": "gnome-shell,extension,blocker,dns,jsdoc,documentation"
    }
  }
}

